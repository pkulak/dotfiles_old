#!/usr/bin/python

import sys
import webbrowser
from urllib.parse import urlparse
from urllib.parse import parse_qs

url = urlparse(sys.argv[1])
id = url.path.split("/")[-1]
query = parse_qs(url.query)
zoom = None

if 'pwd' in query:
    pwd = query['pwd'][0]
    zoom = 'https://zoom.us/wc/{}/join?pwd={}'.format(id, pwd)
else:
    zoom = 'https://zoom.us/wc/{}/join'.format(id)

print(zoom)
webbrowser.open(zoom)
